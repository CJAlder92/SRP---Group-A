#sample data
#here's the bash script I ran. The rn6_refseq.gtf, is the raw copy from UCSC Table, with the output setting as GTF.

#!/bin/bash
cd sample/ &&
mkdir Tophat &&
mkdir Cufflinks &&
cd Tophat &&
/home/bsc10/SRP/Pipeline6/tophat-2.0.0.Linux_x86_64/tophat -r 160 -g 10 -G /home/bsc10/SRP/Pipeline5/Indexes/try2/rn6_refseq.gtf /home/bsc10/SRP/Pipeline5/Indexes/rn6_unmapped /home/bsc10/SRP/Pipeline5/sample/Large_samp1.fq /home/bsc10/SRP/Pipeline5/sample/Large_samp2.fq &&

cd ../Cufflinks &&
cufflinks ../Tophat/tophat_out/accepted_hits.bam 
